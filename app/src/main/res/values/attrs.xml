<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableLayout">
        <attr name="el_duration" format="integer" />
        <attr name="el_expanded" format="boolean" />
        <attr name="el_parallax" format="float" />
        <attr name="android:orientation" />
    </declare-styleable>

    <declare-styleable name="RecyclerView">
        <!--子Item的布局ID-->
        <attr name="itemLayout" format="reference" />
        <!--子Item的布局ID-->
        <attr name="itemsData" format="reference" />
        <!--子Item的布局ID-->
        <attr name="onBindItem" format="reference" />
        <attr name="layoutManager" format="reference" />
    </declare-styleable>

    <declare-styleable name="CustomListView">
        <attr name="textNormalColor" format="color" />
        <attr name="textSelectColor" format="color" />
        <attr name="textBackNormalColor" format="color" />
        <attr name="textBackSelectColor" format="color" />
        <attr name="drawableLeft" format="reference" />
    </declare-styleable>

    <declare-styleable name="DivisionLayoutView">
        <attr name="divisionText" format="string" />
        <attr name="divisionDrawable" format="integer" />
    </declare-styleable>


    <declare-styleable name="GroupTextView">
        <attr name="textLeftColor" format="color" />
        <attr name="textRightColor" format="color" />
        <attr name="textLeftSize" format="dimension" />
        <attr name="textRightSize" format="dimension" />
        <attr name="textLeftString" format="string" />
        <attr name="textRightString" format="string" />
        <attr name="leftDrawable" format="reference" />
        <attr name="rightDrawable" format="reference" />
        <attr name="drawablePaddingLeft" format="dimension" />
        <attr name="drawablePaddingRight" format="dimension" />
        <attr name="leftWidth" format="dimension" />
    </declare-styleable>
</resources>